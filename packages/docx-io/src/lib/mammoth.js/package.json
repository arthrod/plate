{
  "name": "mammoth",
  "version": "1.11.0",
  "author": "Michael Williamson <mike@zwobble.org>",
  "description": "Convert Word documents from docx to simple HTML and Markdown",
  "keywords": [
    "docx",
    "html",
    "office",
    "word",
    "markdown",
    "md"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "browser": "./dist/esm/index.browser.js",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.cjs"
    },
    "./browser": "./dist/mammoth.browser.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mwilliamson/mammoth.js.git"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.6",
    "argparse": "~1.0.3",
    "bluebird": "~3.4.0",
    "dingbat-to-unicode": "^1.0.1",
    "jszip": "^3.10.1",
    "xmlbuilder": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "duck": "^0.1.12",
    "eslint": "2.13.1",
    "hamjest": "^4.0.1",
    "mocha": "~2.2.5",
    "temp": "^0.9.4",
    "tsup": "^8.5.0",
    "typescript": "^5.0.4",
    "vitest": "^4.0.18"
  },
  "browser": {
    "./lib/unzip.js": "./browser/unzip.js",
    "./lib/docx/files.js": "./browser/docx/files.js",
    "./lib/unzip.ts": "./browser/unzip.js",
    "./lib/docx/files.ts": "./browser/docx/files.js"
  },
  "bin": {
    "mammoth": "bin/mammoth"
  },
  "scripts": {
    "test": "bun run build && bun x vitest run --globals",
    "check-typescript": "tsc -p tsconfig.check.json",
    "build": "tsup --config tsup.config.ts && cp dist/esm/index.browser.js mammoth.browser.js && cp dist/mammoth.browser.min.js mammoth.browser.min.js"
  },
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=12.0.0"
  },
  "volta": {
    "node": "16.18.1",
    "npm": "8.19.2"
  }
}
